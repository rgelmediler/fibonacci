package fibonnaci;

import java.util.Scanner;

public class fibo {

	public static void main(String[] args) {
		Scanner yaz = new Scanner(System.in);
		System.out.println("Terim sayısı gir:");
		int say , toplam=0,  a=1 , b=1;
		say=yaz.nextInt();
		if(say==1 || say==2 || say==0) {
			System.out.println(say + ".terim = 1");
		}
		else {
			for(int i=3 ; i<=say ; i++) {
				toplam=a+b;
				
				a=b;
				
				b=toplam;
				
				
			}
			System.out.println(say + "terim" + toplam);
				
		}

	}

}
